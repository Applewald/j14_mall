<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cskaoyan.mall.mapper.AdminMapper">


  <resultMap id="selectAllAdmin" type="com.cskaoyan.mall.bean.AdminResp">
    <result column="id" property="id"/>
    <result column="username" property="username"/>
    <result column="avatar" property="avatar"/>
    <result column="role_ids" property="roleIds" typeHandler="com.cskaoyan.mall.typeHandler.IntArrayTypeHandler"/>
  </resultMap>
  <select id="selectAllAdmin" resultMap="selectAllAdmin">
    select id, username, avatar, role_ids
    from cskaoyan_mall_admin
  </select>
</mapper>